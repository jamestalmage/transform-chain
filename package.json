{
  "name": "transform-chain",
  "version": "0.0.1",
  "description": "Build a chain of transforms",
  "license": "MIT",
  "repository": "jamestalmage/require-wrap",
  "author": {
    "name": "James Talmage",
    "email": "james@talmage.io",
    "url": "github.com/jamestalmage"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "xo && nyc --reporter=lcov --reporter=text ava",
    "markdown": "documentation -c ./docs.json transform-chain.js -f md > foo.md",
    "html": "documentation -c ./docs.json transform-chain.js -o docs -f html",
    "readme": "documentation-readme -s \"API\" -- transform-chain.js",
    "gh-pages": "npm run html && gh-pages -d docs",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "main": "transform-chain.js",
  "files": [
    "transform.js",
    "transform-chain.js"
  ],
  "keywords": [
    ""
  ],
  "dependencies": {
    "fn-name": "^2.0.1",
    "is-regexp": "^1.0.0",
    "multimatch": "^2.1.0"
  },
  "devDependencies": {
    "ava": "github:sindresorhus/ava#8d7e3bb6e73bbea2716b48b5fff59e02c80cb9b1",
    "debug": "^2.2.0",
    "documentation": "^3.0.4",
    "documentation-readme": "^2.1.0",
    "gh-pages": "^0.6.0",
    "nyc": "^4.0.1",
    "xo": "^0.11.2"
  },
  "xo": {
    "ignores": [
      "docs/**",
      "test/*"
    ]
  }
}
